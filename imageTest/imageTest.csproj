        <DebugSymbols>true</DebugSymbols>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Xamarin.Forms" Version="4.3.0.908675" />
        <PackageReference Include="Xamarin.Essentials" Version="1.3.1" />
        <PackageReference Include="Resizetizer.NT" Version="0.1.0-preview8" />
    </ItemGroup>
    <ItemGroup>
        <Folder Include="Assets\" />
    </ItemGroup>
    <ItemGroup>
        <SharedImage Include="Assets\camera.svg" BaseSize="60,60" />
        <SharedImage Include="loginbg.png">
          <BaseSize>60,60</BaseSize>
        </SharedImage>
    </ItemGroup>
    <ItemGroup>
        <None Remove="Assets\camera.svg" />
        <None Remove="loginbg.png" />
    </ItemGroup>
